# Moonbeam Network RPC URLs (you can use the defaults or get your own from providers like OnFinality, BlastAPI, etc.)
MOONBEAM_RPC_URL=https://rpc.api.moonbeam.network
MOONRIVER_RPC_URL=https://rpc.api.moonriver.moonbeam.network
MOONBASE_RPC_URL=https://rpc.api.moonbase.moonbeam.network

# Your wallet private key (for deployment) - NEVER SHARE THIS!
PRIVATE_KEY=your-private-key-here

# Moonscan API Key (get from https://moonscan.io/)
MOONSCAN_API_KEY=your-moonscan-api-key

# Optional: Etherscan API Key (for other networks)
ETHERSCAN_API_KEY=your-etherscan-api-key

# Optional: For gas reporting
COINMARKETCAP_API_KEY=your-coinmarketcap-api-key
